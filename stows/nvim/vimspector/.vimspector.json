{
  "$schema": "https://puremourning.github.io/vimspector/schema/vimspector.schema.json",
  "configurations": {
    "Python - launch": {
      "adapter": "debugpy",
      "configuration": {
        "name": "Python run",
        "type": "python",
        "request": "launch",
        "cwd": "${workspaceRoot}",
        "stopOnEntry": true,
        "console": "integratedTerminal",
        "justMyCode": true,
        "program": "${file}"
      }
    }
  }
}
